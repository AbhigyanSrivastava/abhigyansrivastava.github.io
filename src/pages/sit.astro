---
import aboutImg from "@/assets/about-astro.png";
import PageLayout from "@/layouts/Base.astro";
import { Image } from "astro:assets";
import home1 from '../assets/home.jpg';
import junglefort1 from '../assets/junglefort.jpg';
import nainital1 from '../assets/nainital.jpg';
import manali1 from '../assets/manali.jpg';
import rain1 from '../assets/rain.jpg';
const meta = {
	description: "happily blabbering about myself",
	title: "I want to sit here longer.",
};
---
<script defer src="https://cloud.umami.is/script.js" data-website-id="d8126afa-dc93-427c-a836-8e92a8586a7d"></script>
<PageLayout meta={meta}>
	<div class="space-y-6">
<style>
a:link {
  text-decoration: none;
}

a:visited {
  text-decoration: none;
}

a:hover {
  text-decoration: underline;
}

a:active {
  text-decoration: underline;
}
</style>

<div style="text-align: center;">
  <blockquote>
    But I have promises to keep,<br>
    And miles to go before I sleep.<br>
    Still I want to sit here longer.
  </blockquote>
</div>

<img src={home1.src} alt="no one moves" style="padding: 1px;" />
<div style="text-align: center;">All blue.</div>
<img src={junglefort1.src} alt="pheww" style="padding: 1px;" />
<div style="text-align: center;">village cot</div>
<img src={nainital1.src} alt="no one moves" style="padding: 1px;" />
<div style="text-align: center;">...keep on rowing</div>
<img src={manali1.src} alt="no one moves" style="padding: 1px;" />
<div style="text-align: center;">and we went to Dehradun.</div>
<img src={rain1.src} alt="no one moves" style="padding: 1px;" />
<div style="text-align: center;">...and tea</div>
 <br>

</PageLayout>
